@{
    ViewData["Title"] = "Stwórz Pracowników";
}

<form method="post" asp-controller="Pracownicy" asp-action="CreateNewPracownicy">
  <div class="form-group">
    <label for="id">ID</label>
    <input type="number" class="form-control" id="id" placeholder="ID" name="id">
  </div>
  <div class="form-group">
    <label for="name">Name</label>
    <input type="text" class="form-control" id="name" placeholder="Name" name="name">
  </div>
  <div class="form-group">
    <label for="name">Dzial</label>
    <input type="text" class="form-control" id="dzial" placeholder="Dzial" name="dzial">
  </div>
    <div class="form-group">
    <label for="name">Gender</label>
    <input type="number" class="form-control" id="gender" placeholder="Gender" name="gender">
  </div>
  <button type="submit" class="btn btn-primary">Submit</button>
</form>
